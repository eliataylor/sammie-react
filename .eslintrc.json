{
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "commonjs": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
     "plugins": [
       "react"
    ],
    "rules": {
      "indent": 0,
      "no-tabs": 0,
      "key-spacing":0,
      "no-console":0,
      "no-unused-vars":0,
      "no-debugger":0
    }
}
